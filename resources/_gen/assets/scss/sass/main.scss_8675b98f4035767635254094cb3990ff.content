html,body,h1,h2,h3,h4,h5,h6,p,ol,ul{margin:0;padding:0;border:0}article,aside,details,figcaption,figure,footer,header,hgroup,main,menu,nav,section{display:block}img{border-style:none}body{line-height:1;font-size:16px;font-family:Arial,sans-serif}ol,ul{list-style:none}a{background-color:transparent;text-decoration:none}input,textarea,button,select,a{-webkit-tap-highlight-color:transparent}*,*:before,*:after{box-sizing:border-box}table{border-collapse:collapse;border-spacing:0}.flex-container{display:flex;flex-wrap:wrap;gap:20px}.flex-item{max-width:calc(100% - 20px);display:flex;flex-direction:column}img{max-width:100%;height:auto}.image-container{display:flex;flex-wrap:nowrap;gap:10px}.image-item{position:relative}.image-item img{width:100%;height:auto;max-width:300px;margin:0 auto}.image-item .project-content{width:100%;max-width:300px;box-sizing:border-box;margin:0 auto}.no-underline{text-decoration:initial}@font-face{font-family:aulac;src:url(fonts/aulac/AulacRegular.woff2)format("woff2"),url(fonts/aulac/AulacRegular.woff)format("woff");font-weight:400;font-style:normal}.image-caption{font-family:aulac;font-kerning:auto;hyphens:none;letter-spacing:.005em;text-align:center;font-size:.8em;padding-bottom:1em;padding-top:.5em}.project-content,.bio{font-family:aulac;font-kerning:auto;hyphens:none;letter-spacing:.005em;text-align:left;font-size:1em;line-height:1.25em}body.frozen{overflow:hidden}